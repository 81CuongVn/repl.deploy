        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug",
            "program": "${workspaceFolder}/daemon/rust-impl/target/debug/repl_deploy",
            "args": [
                "deno",
                "run",
                "-A",
                "--inspect-brk",
                "server.ts"
